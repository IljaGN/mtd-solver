<Application x:Class="MTD_Solver.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="View\MainWindow.xaml"
             Startup="Application_Startup"
             Exit="Application_Exit">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View/Images/CocurrentExchanger.xaml"/>
                <ResourceDictionary Source="View/Images/CountercurrentExchanger.xaml"/>
                <ResourceDictionary Source="View/Images/ShellAndTubeExchanger.xaml"/>
                <ResourceDictionary Source="View/Images/CrossFlowExchanger.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
