<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ControlTemplate x:Key="ShellAndTubeExchanger">
        <Canvas Width="150" Height="50">
            <Rectangle Stroke="Black" StrokeThickness="3" Width="150" Height="50"/>
            <Line Stroke="Black" StrokeThickness="3" X1="120.5" Y1="0" X2="120.5" Y2="30"/>
            <Line Stroke="Black" StrokeThickness="3" X1="60.5" Y1="0" X2="60.5" Y2="30"/>
            <Line Stroke="Black" StrokeThickness="3" X1="90.5" Y1="20" X2="90.5" Y2="50"/>
            <Line Stroke="Black" StrokeThickness="3" X1="30.5" Y1="20" X2="30.5" Y2="50"/>
            <Line Stroke="Black" StrokeThickness="2" X1="20" Y1="15" X2="170" Y2="15"/>
            <Line Stroke="Black" StrokeThickness="2" X1="20" Y1="35" X2="170" Y2="35"/>
            <Path Stroke="Black" StrokeThickness="2">
                <Path.Data>
                    <PathGeometry>
                        <PathFigure StartPoint="20, 15" IsClosed="False">
                            <ArcSegment Point="20, 35" Size="1, 1"/>
                        </PathFigure>
                    </PathGeometry>
                </Path.Data>
            </Path>
        </Canvas>
    </ControlTemplate>
</ResourceDictionary>
