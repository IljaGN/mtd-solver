<Window x:Class="MTD_Solver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MTD_Solver"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <WrapPanel Grid.Column="0" Grid.Row="0">
            <StackPanel Orientation="Horizontal">
                <Label>T1</Label>
                <TextBox MinWidth="80"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Margin="20 0 0 0">T2</Label>
                <TextBox MinWidth="80"></TextBox>
            </StackPanel>
        </WrapPanel>
        <WrapPanel Grid.Column="1" Grid.Row="0">
            <Label>t1</Label>
            <TextBox MinWidth="80"></TextBox>
            <Label Margin="20 0 0 0">t2</Label>
            <TextBox MinWidth="80"></TextBox>
        </WrapPanel>
        <WrapPanel Grid.Column="0" Grid.Row="1">
            <Label>P</Label>
            <TextBox MinWidth="80"></TextBox>
            <Label Margin="20 0 0 0">R</Label>
            <TextBox MinWidth="80"></TextBox>
        </WrapPanel>
        <WrapPanel Grid.Column="1" Grid.Row="1">
            <Label>mtd</Label>
            <TextBox MinWidth="80"></TextBox>
            <Label Margin="20 0 0 0">F</Label>
            <TextBox MinWidth="80"></TextBox>
        </WrapPanel>
        <WrapPanel Grid.Column="2" Grid.Row="1">
            <Label>mtd*F</Label>
            <TextBox MinWidth="80"></TextBox>
        </WrapPanel>
    </Grid>
</Window>
